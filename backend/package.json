{
  "name": "gemini-cli-ui-backend",
  "version": "1.0.0",
  "description": "Backend server for Gemini CLI UI",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "keywords": [
    "express",
    "typescript",
    "websocket",
    "gemini",
    "ide",
    "backend"
  ],
  "author": "Gemini CLI UI Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4",
    "prisma": "^5.6.0",
    "@prisma/client": "^5.6.0",
    "chokidar": "^3.5.3",
    "node-pty": "^1.0.0",
    "simple-git": "^3.20.0",
    "mime-types": "^2.1.35",
    "uuid": "^9.0.1",
    "ws": "^8.14.2",
    "axios": "^1.6.2",
    "js-yaml": "^4.1.0",
    "multer": "^1.4.5-lts.1",
    "rate-limiter-flexible": "^4.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@types/mime-types": "^2.1.4",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@types/js-yaml": "^4.0.9",
    "@types/multer": "^1.4.11",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "tsx": "^4.1.4",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
